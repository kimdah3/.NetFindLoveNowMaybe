@model FindLoveNowMaybe.Models.HomeModel
@{
    ViewBag.Title = "IndexUser";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>Profiler som redan är medlemmar!</h2>


<div class="container">
    <div class="jumbotron text-center">
        <h1>Find Love Now Maybe</h1>
        <p>Welcome lover</p>
        @Html.ActionLink("Go to profile", "Profile", "User", new {username = User.Identity.Name}, new { @class = "btn btn-primary" })

    </div>
</div>
<!--Några profiler-->
<div class="row">
    <div class="col-md-3 col-sm-6 hero-feature">
