            <a href="#">
                <img src="~/Content/Images/@Model.RandomUsers[0].Picture" class="img-rounded" alt="hej">

            </a>
            <div class="caption">
                <h3>@Model.RandomUsers[0].UserName </h3>
                <p>Hej hej</p>
                <p>
                    <a href="/home/register" class="btn btn-info">See profile</a>
                </p>
            </div>
        </div>
    </div>

